<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="#5d26c1"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp" />
                <gradient
                    android:startColor="#80ffd3"
                    android:centerColor="#80ffb6"
                    android:centerY="0.75"
                    android:endColor="#a0ffcb"
                    android:angle="0"
                    />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient
                    android:startColor="#59c173"
                    android:centerColor="@color/color_schedule_yellow"
                    android:endColor="#59c173"
                    />
            </shape>
        </clip>
    </item>

</layer-list>